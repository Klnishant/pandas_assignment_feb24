    "import pandas as pd\n",
    "data = {\"Date\":[\"2023-01-01\",\"2023-01-02\",\"2023-01-03\",\"2023-01-04\",\"2023-01-05\"]}\n",
    "df = pd.DataFrame(data)\n",
    "df[\"Date\"] = pd.to_datetime(df[\"Date\"])\n",
    "df['Weekday'] = df['Date'].dt.strftime('%A')\n",
    "df"
   ]
